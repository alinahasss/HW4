#1
import math

d = input('Введите степень округления в формате 0.01 от 10 в степени -1 до 10 в степени -10 -> ')
d = d[2:len(d)]
print(round(math.pi,len(d)))

#2
num = int(input("Введите число: "))
i = 2 
lst = []
old = num
while i <= num:
    if num % i == 0:
        lst.append(i)
        num //= i
        i = 2
    else:
        i += 1
print(lst)

#3
a = [1, 3, 5, 1, 3, 4, 3]
print(a)
l = [] 
for i in a:
    if i not in l:
        l.append(i)
print(l) 
